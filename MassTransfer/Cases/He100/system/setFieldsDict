FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(
    boxToCell
    {
        box (-1 0 -1) (1 0.005 1);
        fieldValues
        (
            volScalarFieldValue alpha.water 1
	    volScalarFieldValue H 1
	    volScalarFieldValue M 1
        );
    }
);


